<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
<link rel="shortcut icon" href="/assets/images/favicon.png" type="image/png">
        <!-- animate scss -->
        <link rel="stylesheet" href="/assets/css/animate.css">
        <!-- bootstarp css -->
        <link rel="stylesheet" href="/assets/css/bootstrap.min.css">
        <!-- icofont -->
        <link rel="stylesheet" href="/assets/css/icofont.min.css">
        <!-- lightcase css -->
        <link rel="stylesheet" href="/assets/css/lightcase.css">
        <!-- swiper css -->
        <link rel="stylesheet" href="/assets/css/swiper.min.css">
        <!-- cusyom scss -->
        <link rel="stylesheet" href="/assets/css/style.css">

    <link rel="stylesheet" href="/assets/css/mobile.responsive.css">
        <link href="https://fonts.googleapis.com/css2?family=Quicksand:wght@300;400;500;600;700&display=swap" rel="stylesheet">
        
        <title>Boozly - Alcoholic beverages delivered to your door within 60 minutes</title>
    </head>

<body>		<!-- preloader -->
        
    <!-- preloader -->
    <div class="preloader">
        <div class="load loade">
            <hr/>
            <hr/>
            <hr/>
            <hr/>
        </div>
    </div>
    <!-- preloader -->

    <!-- Mobile Menu Start Here -->
    <% include ../partials/header %>
        <!-- header section ending -->



        <!-- Login Section Start Here -->
        <section class="contact-information login logsign-section padding-tb">
            <div class="container">
                <div class="section-wrapper">
                    <div class="row">

                        <div class="col-lg-6 offset-lg-3">
                            <div role="tabpanel">


                                <ul class="nav nav-tabs logsign" role="tablist">
                                  <li class="nav-item">
                                    <a class="nav-link<%if(!register){%> active<%}%>" data-toggle="tab" id="log" href="/#logIn" role="tab">Login</a>
                                  </li>
                                  <li class="nav-item">
                                    <a class="nav-link <%if(register){%> active<%}%>" data-toggle="tab" id="sign" href="/#signUp" role="tab">Sign Up</a>
                                  </li>
                                </ul>


                                <!-- Tab panes -->
                              <div class="tab-content">
                                <div role="tabpanel" class="tab-pane <%if(!register){%>active<%}%>" id="logIn">
                                  <div class="login-section">
                                 <form id="login-form" action="/boozly/login" method="post" novalidate="novalidate">

                                      <div class="form-group">
                                   <input type="email" name="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Your Email">     <span class="symbol-input100">
                                            <span class="lnr lnr-envelope"></span>
                                        </span>
                                      </div>

                                      <div class="form-group">
                                    <input type="password" name="md5password" class="form-control" id="exampleInputPassword1" placeholder="Your Password">    <span class="symbol-input100">
                                            <span class="lnr lnr-lock"></span>
                                        </span>
                                      </div>
                                      <div class="form-check">
                                          <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
                                          <label class="form-check-label" for="defaultCheck1">Remember me</label>
                                          <a id="forgot" href="/boozly/forgetpassword">Forgot Password?</a>
                                        </div>
                                      <button type="submit" class="food-btn"><span>Login</span></button>
  	<%   if(typeof FlashMsg != 'undefined' && FlashMsg) { %>
							<label id="loginmsg" class="error" ><%- FlashMsg %></label>
							<% } %>

                                     <div class="text-center"><strong><a style="color: #fb524f;" href="/boozly">Back to Home</a></strong></div>


                                    </form>
                                    </div>
                                  </div>

                                  <div role="tabpanel" class="tab-pane <%if(register){%>active<%}%> " id="signUp">
                                    <div class="register-section">
                                        <form id="register-form" action="/boozly/register" method="post" novalidate="novalidate">
                      
                                          <div class="form-group row">
                                            <div class="col-sm-6">
                                                 <input type="text" class="form-control txtOnly" <%if(oldData!=''){%>value="<%-oldData.fname%>" <%}%> name="fname" aria-describedby="" placeholder="Your First Name">
                                                <span class="symbol-input110 fname">
                                                    <span class="lnr lnr-user"></span>
                                                </span>
                                            </div>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control txtOnly" <%if(oldData!=''){%>value="<%-oldData.lname%>" <%}%> name="lname" aria-describedby="" placeholder="Your Last Name">
                                                <span class="symbol-input110 lname">
                                                    <span class="lnr lnr-user"></span>
                                                </span>
                                            </div>
                                          </div>

                                          <div class="form-group">
                                               <input type="email" class="form-control" name="email" <%if(oldData!=''){%>value="<%-oldData.email%>" <%}%> aria-describedby="emailHelp" placeholder="Your Email">
                                                <span class="symbol-input110">
                                                    <span class="lnr lnr-envelope"></span>
                                                </span>
                                          </div>
                                          <div class="form-group">

                                    <input type="password" class="form-control" id="password" <%if(oldData!=''){%>value="<%-oldData.text_password%>" <%}%> aria-describedby="emailHelp" name="text_password" placeholder="Your Password">
                                                <span class="symbol-input110">
                                                    <span class="lnr lnr-lock"></span>
                                                </span>
                                          </div>
                                          <div class="form-group">
   <input type="password" class="form-control" name="password" <%if(oldData!=''){%>value="<%-oldData.text_password%>" <%}%> id="password2" aria-describedby="" placeholder="Confirm Password">
                                                <span class="symbol-input110">
                                                    <span class="lnr lnr-lock"></span>
                                                </span>
                                          </div>
                                            <div class="form-check">
                                              <input class="form-check-input" type="checkbox" value="" id="defaultCheck2">
                                              <label class="form-check-label" for="defaultCheck2">I consent to my personal data being processed for the purpose of fulfilling this agrement, as outlined in the <a style="color: #fb524f;" href="/">Privacy Policy</a></label>
                                            </div>
                                            <div class="form-check">
                                              <input class="form-check-input" type="checkbox" value="" id="defaultCheck3">
                                              <label class="form-check-label" for="defaultCheck3">I have read, understood & agree to the <a style="color: #fb524f;" href="/">Terms and Conditions</a></label>
                                            </div>

                                          <button type="submit" class="food-btn"><span>Sign Up</span></button>
                                          	<%   if(typeof FlashMsg != 'undefined' && FlashMsg) { %>
							<label id="rgstrmsg" class="error" ><%- FlashMsg %></label>
							<% } %>

                                         

                                          <div class="text-center"><strong><a style="color: #fb524f;" href="/">Back to Home</a></strong></div>

                                          


                                        </form>
                                    </div>
                                  </div>


                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </section>
        <!-- Contact Us Section Ending Here -->




		<!-- Footer Section Start Here -->

     

    <% if (ISMOBILE === "1") { %>
      <!-- MOBILE TAB MENU Here -->
          <% include ../partials/tabBar %>
      <% } else { %>
          <% include ../partials/footer %>
      <% } %>
      
        <!-- Footer Section Ending Here -->
        
        <!-- scrollToTop start here -->
		<a href="/#" class="scrollToTop"><i class="icofont-swoosh-up"></i></a>
		<!-- scrollToTop ending here -->


		
		<script src="/assets/js/jquery.js"></script>
		<script src="/assets/js/waypoints.min.js"></script>
		<script src="/assets/js/bootstrap.min.js"></script>
		<script src="/assets/js/isotope.pkgd.min.js"></script>
		<script src="/assets/js/wow.min.js"></script>
		<script src="/assets/js/swiper.min.js"></script>
		<script src="/assets/js/lightcase.js"></script>
		<script src="/assets/js/jquery.counterup.min.js"></script>
        <script src="/assets/js/functions.js"></script>
              
        <script src="/assets/js/jquery_validate.js"></script>
    
        <script src="/assets/js/validation.js"></script>
        <script>
        

$('#sign').on('click', function(){
    $('#logIn').removeClass('active');
    $('#signUp').addClass('active');
      $('#loginmsg').remove();
        $('#rgstrmsg').remove();
});
   $('#log').on('click', function(){
    
    $('#signUp').removeClass('active');
    $('#logIn').addClass('active');
         $('#loginmsg').remove();
          $('#rgstrmsg').remove();
});
        </script>
	</body>
</html>

<% include ../partials/activeclass %>
<% include ../partials/location %>