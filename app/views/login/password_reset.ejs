<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />

    <!-- favicon -->
    <link
      rel="shortcut icon"
      href="/assets/images/favicon.png"
      type="image/png"
    />
    <!-- animate scss -->
    <link rel="stylesheet" href="/assets/css/animate.css" />
    <!-- bootstarp css -->
    <link rel="stylesheet" href="/assets/css/bootstrap.min.css" />
    <!-- icofont -->
    <link rel="stylesheet" href="/assets/css/icofont.min.css" />
    <!-- lightcase css -->
    <link rel="stylesheet" href="/assets/css/lightcase.css" />
    <!-- swiper css -->
    <link rel="stylesheet" href="/assets/css/swiper.min.css" />
    <!-- cusyom scss -->
    <link rel="stylesheet" href="/assets/css/style.css" />
    <link
      href="https://fonts.googleapis.com/css2?family=Josefin+Sans:wght@400;500;600;700&display=swap"
      rel="stylesheet"
    />
    <link
      href="https://fonts.googleapis.com/css2?family=Quicksand:wght@300;400;500;600;700&display=swap"
      rel="stylesheet"
    />

    <title>
      Boozly - Alcoholic beverages delivered to your door within 60 minutes
    </title>
  </head>

  <body>
    <!-- preloader -->
    <div class="preloader">
      <div class="load loade">
        <hr />
        <hr />
        <hr />
        <hr />
      </div>
    </div>
    <!-- preloader -->
    <% include ../partials/header %>
    <!-- header section ending -->

    <!-- Login Section Start Here -->
    <section class="contact-information login logsign-section padding-tb">
      <div class="container">
        <div class="section-wrapper">
          <div class="row">
            <div class="col-lg-6 offset-lg-3">
              <h5>Reset Password</h5>
              <div class="login-section">
                <form
                  form
                  id="register-form"
                  action="/boozly/resetpassword"
                  method="post"
                  novalidate="novalidate"
                >
                  <div class="form-group">
                    <input type="hidden" value="<%-token%>" name="token" />
                    <label for="">Your New Password</label>
                    <div class="position-relative">
                      <input
                        type="password"
                        class="form-control"
                        id="password"
                        aria-describedby="emailHelp"
                        name="text_password"
                        placeholder=""
                      />
                      <span class="symbol-input110"
                        ><span class="lnr lnr-lock"></span
                      ></span>
                    </div>
                  </div>
                  <div class="form-group">
                    <label for="">Confirm Your New Password</label>
                    <div class="position-relative">
                      <input
                        type="password"
                        class="form-control"
                        name="password"
                        id="password2"
                        aria-describedby=""
                        placeholder=""
                      />
                      <span class="symbol-input110">
                        <span class="lnr lnr-lock"></span>
                      </span>
                      <span id="pass_info" style="padding-bottom: 20px"></span>
                    </div>
                  </div>

                  <div class="form-check">
                    <a id="forgot" href="/boozly/login">Login</a>
                  </div>
                  <button type="submit" class="food-btn">
                    <span>Submit</span>
                  </button>

                  <div class="signup-link text-center">
                    New to Boozly?
                    <strong
                      ><a style="color: #fb524f" href="/boozly/register">
                        Sign up!</a
                      ></strong
                    >
                  </div>

                  <div class="text-center">
                    <strong
                      ><a style="color: #fb524f" href="/boozly"
                        >Back to Home</a
                      ></strong
                    >
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
    </section>
    <!-- Contact Us Section Ending Here -->

    <!-- Footer Section Start Here -->
    <% if (ISMOBILE === "1") { %>
      <!-- MOBILE TAB MENU Here -->
          <% include ../partials/tabBar %>
      <% } else { %>
          <% include ../partials/footer %>
      <% } %>
    <!-- Footer Section Ending Here -->

    <!-- scrollToTop start here -->
    <a href="javascript:void(0);" class="scrollToTop"
      ><i class="icofont-swoosh-up"></i
    ></a>
    <!-- scrollToTop ending here -->

    <script src="/assets/js/jquery.js"></script>
    <script src="/assets/js/waypoints.min.js"></script>
    <script src="/assets/js/bootstrap.min.js"></script>
    <script src="/assets/js/isotope.pkgd.min.js"></script>
    <script src="/assets/js/wow.min.js"></script>
    <script src="/assets/js/swiper.min.js"></script>
    <script src="/assets/js/lightcase.js"></script>
    <script src="/assets/js/jquery.counterup.min.js"></script>
    <script src="/assets/js/functions.js"></script>
    <script src="/assets/js/jquery_validate.js"></script>

    <script src="/assets/js/validation.js"></script>
  </body>
</html>
